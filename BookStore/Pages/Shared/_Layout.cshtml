<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@ViewData["Title"] - BookStore</title>
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <link rel="stylesheet" href="~/css/site.css" />
  <link rel="stylesheet" href="~/lib/materialaze/css/materialize.min.css">
  <script type="text/javascript" src="~/lib/jquery/dist/jquery.js"></script>
  <script type="text/javascript" src="~/lib/angular.min.js"></script>
</head>


<body>
<header>

 <nav>
    <div class="nav-wrapper brown lighten-4">
      <a href="#!" class="brand-logo center">Logo</a>
      <ul class="left hide-on-med-and-down">
        <li><a href="index">Home</a></li>
        <li><a href="profile">Profile</a></li>
        <li class=""><a href="login">Login</a></li>
        <li><a href="Admin">Admin</a></li>
      </ul>
    </div>
  </nav>

</header>







        <main role="main" class="pb-3">
          @RenderBody()
        </main>





<footer class="page-footer  brown lighten-4">
  <div class=" wrapper_wrap">
    <div class="row">
      <div class="col l6 s12">
        <h5 class="white-text">Footer Content</h5>
        <p class="grey-text text-lighten-4">You can use rows and columns here to organize your footer content.</p>
      </div>
      <div class="col l4 offset-l2 s12">
        <h5 class="white-text">Links</h5>
        <ul>
          <li><a class="grey-text text-lighten-3" href="#!">Link 1</a></li>
          <li><a class="grey-text text-lighten-3" href="#!">Link 2</a></li>
          <li><a class="grey-text text-lighten-3" href="#!">Link 3</a></li>
          <li><a class="grey-text text-lighten-3" href="#!">Link 4</a></li>
        </ul>
      </div>
    </div>
      </div>
  <div class="footer-copyright">
      © 2014 Copyright Text
      <a class="grey-text text-lighten-4 right" href="#!">More Links</a>
  </div>

</footer>



<script type="text/javascript" src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/lib/materialaze/js/materialize.min.js"></script>
<script type="text/javascript" src="~/js/site.js" asp-append-version="true"></script>

@RenderSection("Scripts", required: false)

</body>
</html>
